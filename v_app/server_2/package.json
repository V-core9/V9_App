{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon --legacy-watch ./app.js",
    "lint": "eslint --fix src",
    "test": "mocha --exit",
    "prisma:deploy": "sh -c \" npm run prisma:reset ; npm run prisma:migrate ; npm run prisma:seed \"",
    "prisma:migrate": "npx prisma migrate dev ",
    "prisma:seed": "node prisma/seed.js",
    "prisma:reset": "echo \"y\"|npx prisma migrate reset"
  },
  "dependencies": {
    "@prisma/client": "^3.15.2",
    "bcrypt": "^5.0.1",
    "clean-css": "^5.3.1",
    "compression": "^1.7.4",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "~4.16.1",
    "helmet": "^4.6.0",
    "html-minifier": "^4.0.0",
    "html-minifier-terser": "^7.0.0",
    "http-errors": "~1.6.3",
    "jsonminify": "^0.4.2",
    "jsonwebtoken": "^8.5.1",
    "minify-xml": "^3.4.0",
    "morgan": "^1.10.0",
    "prisma": "^3.15.2",
    "pug": "2.0.0-beta11",
    "swagger-jsdoc": "5.0.1",
    "swagger-ui-express": "^4.5.0",
    "terser": "^5.15.0",
    "uuid": "^8.3.2",
    "v_to_sha256": "^1.1.0",
    "x-powered-by-random": "^0.0.4",
    "socket.io": "^4.5.1"
  },
  "devDependencies": {
    "eslint": "^7.28.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.4",
    "mocha": "^9.0.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3"
  }
}
